<!DOCTYPE html>
<html>
  <head>
    <title><%= Obelisk.Config.config.name %></title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="http://fonts.googleapis.com/css?family=Raleway" rel='stylesheet' type='text/css' />
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <%= @css %>
    <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', '<%= Obelisk.Config.config.ga %>', 'auto');
    ga('send', 'pageview');
  </script>
  </head>

  <body>
    <div class="header">
      <div class="container">
        <div class="six columns alpha">
          <h1><%= Obelisk.Config.config.name %></h1>
          <h2><%= Obelisk.Config.config.description %></h2>
        </div>
        <div class="six columns omega">

        </div>
      </div>
    </div>

    <div class="main">
      <div class="container">
        <div class="nine columns alpha">
          <%= @content %>
        </div>
        <div class="three columns omega">
          <p class="about">
            <%= Obelisk.Config.config.bio %>
          </p>
          <p class="find-me centered">
            <a href="<%= Obelisk.Config.config.twitter %>" target="_"><i class="fa fa-twitter fa-2x"></i></a>
            <a href="<%= Obelisk.Config.config.github %>" target="_"><i class="fa fa-github fa-2x"></i></a>
          </p>
        </div>
      </div>
    </div>
  </body>
</html>
